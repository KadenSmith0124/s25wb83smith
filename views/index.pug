extends layout

block content
  h1= title
  p AHHHHHHHHH Welcome to #{title}
  a.btn(href='#') Press
  p styled button
  button(type="button", class="btn btn-secondary") Left
  div#faster Add in extra

script.
      let value = 1;
      let amt = 2;

      function faster() {
        amt *= 2;
        value += amt;
        document.getElementById("faster").innerText = `Faster ${value} times ${amt}`;
      }
